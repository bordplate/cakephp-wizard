{
    "name": "bordplate/cakephp-wizard",
    "description": "CakePHP3 Wizard Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "wizard"],
    "homepage": "https://github.com/ProLoser/CakePHP-Wizard",
    "license": "MIT",
    "authors": [
        {
            "name": "Dean Sofer",
            "homepage": "https://github.com/ProLoser",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wizard\\": "src"
        }
    }
    "replace": {
        "lucasff/cakephp-wizard": "dev-master",
        "ProLoser/CakePHP-Wizard": "dev-master"
    },
    "require": {
        "php": ">=7.0",
        "composer/installers": "*"
    },
    "extra": {
        "installer-name": "Wizard"
    }
}
